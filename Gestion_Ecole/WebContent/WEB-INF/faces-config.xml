<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">


	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>navAide</from-outcome>
			<to-view-id>/aide.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>




	<navigation-rule>
		<from-view-id>/aide.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>navIndex</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>


	<navigation-rule>
		<from-view-id>/gestion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listAdmin</from-outcome>
			<to-view-id>/gestion.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>

</faces-config>
